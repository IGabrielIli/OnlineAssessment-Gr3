@startuml ADOPSE_uml
!theme toy
frame "Front end" {
  class User {
    -int id
    -String name
    -String email
    -Role role
  }
  class Question {
    -int id
    -String questionText
    -int answersId
    -QuestionType type
    -int difficulty
    -int ownerUserId
    -int examId
  }
  class Exam {
    -int id
    -int examId
    -String examName
  }
  class Answers {
    -int id
    -String answerText
    -bool isCorrect
  }
  class QuestionAnswers {
    -int questionId
    -int answerId
  }
  Answers -- QuestionAnswers
  Question -- QuestionAnswers
  Question -- Exam
}
@enduml